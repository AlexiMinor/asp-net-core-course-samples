@model NewsAggregator.DAL.Core.Entities.RssSourse

@{
    ViewData["Title"] = "View";
}

<h1>View</h1>

<h4>RssSourse</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        @using (Html.BeginForm("Create", "RssSourses", FormMethod.Post))
        {
            <div>
                @Html.LabelFor(model => model.Name)
                @Html.TextBoxFor(sourse => sourse.Name)
            </div>
            <input type="submit"/>
        }
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
